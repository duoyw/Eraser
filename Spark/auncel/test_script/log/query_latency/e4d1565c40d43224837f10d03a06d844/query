SELECT     COUNT(*) FROM     comments as c,     posts as p,     postHistory as ph,     badges as b,     users as u WHERE     u.Id = ph.UserId     AND u.Id = b.UserId     AND u.Id = p.OwnerUserId     AND u.Id = c.UserId     AND c.Score = 2     AND p.PostTypeId = 2     AND p.ViewCount >= 2     AND p.ViewCount <= 6000     AND p.FavoriteCount = 0     AND p.CreationDate <= '2014-12-11 09:58:16' :: timestamp;