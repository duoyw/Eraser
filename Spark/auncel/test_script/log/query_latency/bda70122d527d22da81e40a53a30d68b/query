select  count(*) from comments as c,    posts as p,          postHistory as ph,          votes as v,          badges as b,          users as u  where u.Id = p.OwnerUserId     and u.Id = b.UserId     and p.Id = c.PostId     and p.Id = ph.PostId     and p.Id = v.PostId  AND c.Score=0  AND ph.PostHistoryTypeId=5  AND ph.CreationDate<='2014-09-09 22:39:54'::timestamp  AND p.Score>=-1  AND p.Score<=28  AND p.ViewCount<=5924  AND u.Reputation<=344;