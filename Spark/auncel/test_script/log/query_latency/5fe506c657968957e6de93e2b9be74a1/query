select  count(*) from comments as c,    posts as p,           votes as v,    badges as b,          users as u  where u.Id =c.UserId  and c.UserId = p.OwnerUserId   and p.OwnerUserId = v.UserId  and v.UserId = b.UserId  AND p.PostTypeId=1  AND p.Score>=-2  AND p.CreationDate>='2010-07-31 05:49:03'::timestamp  AND u.Reputation>=1  AND u.DownVotes<=143  AND u.UpVotes>=0  AND u.UpVotes<=3274  AND v.BountyAmount<=100  AND v.CreationDate>='2010-07-19 00:00:00'::timestamp;