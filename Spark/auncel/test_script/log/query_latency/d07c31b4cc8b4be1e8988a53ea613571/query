select  count(*) from postHistory as ph,          votes as v,    users as u,    badges as b  where u.Id = ph.UserId  and u.Id = v.UserId  and u.Id = b.UserId  AND u.Views>=0  AND u.Views<=85  AND u.DownVotes>=0  AND u.DownVotes<=1  AND v.VoteTypeId=5  AND v.CreationDate>='2010-07-19 00:00:00'::timestamp;